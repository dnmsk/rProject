@using Project_B.Code
@model MainLogic.WebFiles.BaseModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - RichBet.ru</title>
    @SquishItMinifierStatic.Css.MvcRenderCachedAssetTag()
    @SquishItMinifierStatic.JavaScript.MvcRenderCachedAssetTag()

</head>
<body>
    @Html.Partial("_MainMenu", Model)
    <div class="container body-content">
        @RenderBody()
        <hr/>
        <footer>
            <p>&copy; @DateTime.Now.Year - dnmsk</p>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>
