@using CommonUtils.ExtendedTypes
@model List<Project_B.CodeClientSide.TransportType.ModerateTransport.RawEntityWithLink>

@foreach (var entity in Model) {
    <label data-uniqueid="@entity.EntityID" data-type="@entity.BrokerEntityType" data-tooltip="true">
        <input type="checkbox" value="@entity.EntityID" name="ids"/>
        @entity.EntityName.StrJoin(" | ") <b>ID=@entity.EntityID</b>
    </label>
    <br/>
}